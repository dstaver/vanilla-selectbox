{
  "name": "@startsiden/lib-adloader",
  "description": "Helper library to configure and load ads from DFP (Doubleclick For Publishers).",
  "author": "Daniel Staver <daniel@petraflux.com>",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/startsiden/lib-adloader2.git"
  },
  "bugs": {
    "url": "https://github.com/startsiden/lib-adloader2/issues"
  },
  "homepage": "https://github.com/startsiden/lib-adloader2",
  "main": "dist/adloader.cjs.js",
  "module": "dist/adloader.esm.js",
  "browser": "dist/adloader.umd.js",
  "husky": {
    "hooks": {
      "pre-commit": "run-p test lint",
      "pre-push": "run-p test lint md",
      "post-merge": "yarn"
    }
  },
  "scripts": {
    "md": "jsdoc2md --template README.hbs --files './src/adloader/**/*.js' > README.md",
    "start": "run-s build dev browsersync",
    "browsersync": "browser-sync start --config browsersync.config.js",
    "dev": "run-p dev:*",
    "dev:js": "rollup -c rollup.config.dev.js",
    "dev:static": "cpx './src/static/**' dist",
    "clean": "run-s clean:*",
    "clean:dist": "rm -rf dist/*",
    "clean:doc": "rm -rf docs/*",
    "postclean": "mkdir -p docs dist",
    "prebuild": "run-s clean",
    "build": "run-p build:*",
    "build:js": "rollup -c",
    "build:doc": "jsdoc -c jsdoc.json",
    "prepublish": "run-s test lint build",
    "watch": "run-p watch:* browsersync",
    "watch:js": "rollup --watch -c rollup.config.dev.js",
    "watch:doc": "chokidar './src/adloader/**/*.js' -c 'npm run build:doc'",
    "watch:static": "cpx './src/static/**' dist -w",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "NODE_ENV=test jest --coverage"
  },
  "dependencies": {
    "element-ready-es5": "^2.1.0",
    "husky": "^0.15.0-rc.3",
    "rambda": "^1.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.38",
    "@babel/plugin-external-helpers": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "babel-cli": "^7.0.0-beta.3",
    "babel-core": "^7.0.0-beta.3",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "browsersync": "^0.0.1-security",
    "chokidar-cli": "^1.2.0",
    "cpx": "^1.5.0",
    "docdash": "^0.4.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-prettier": "^2.5.0",
    "jest": "^22.1.4",
    "jest-css-modules": "^1.1.0",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "rollup": "^0.55.1",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-browsersync": "^0.2.6",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-sizes": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
