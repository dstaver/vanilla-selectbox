{
  "name": "vanilla-selectbox",
  "description": "VanillaJS Selectbox Component",
  "author": "Daniel Staver <daniel@petraflux.com>",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dstaver/vanilla-selectbox.git"
  },
  "bugs": {
    "url": "https://github.com/dstaver/vanilla-selectbox/issues"
  },
  "homepage": "https://github.com/dstaver/vanilla-selectbox",
  "main": "dist/vanilla-selectbox.cjs.js",
  "module": "dist/vanilla-selectbox.esm.js",
  "browser": "dist/vanilla-selectbox.umd.js",
  "scripts": {
    "precommit": "run-p test lint",
    "prepush": "run-p test lint doc:md",
    "postmerge": "yarn",
    "prepublishOnly": "run-s test lint build doc:md",
    "start": "run-s build dev browsersync",
    "browsersync": "browser-sync start --config browsersync.config.js",
    "dev": "run-p dev:*",
    "dev:js": "rollup -c rollup.config.dev.js",
    "dev:static": "cpx './src/static/**' dist",
    "clean": "run-s clean:*",
    "clean:dist": "rm -rf dist/*",
    "clean:doc": "rm -rf docs/*",
    "postclean": "mkdir -p docs dist",
    "prebuild": "run-s clean",
    "build": "run-p build:*",
    "build:js": "rollup -c",
    "build:doc": "run-s doc",
    "doc": "run-p doc:*",
    "doc:html": "jsdoc -c jsdoc.json",
    "doc:md": "cat README.tpl.md > README.md && jsdoc2md --template README.hbs --files './src/**/*.js' >> README.md",
    "prewatch": "run-s clean",
    "watch": "run-p doc watch:* browsersync",
    "watch:js": "rollup --watch -c rollup.config.dev.js",
    "watch:doc": "sane 'npm run build:doc' --glob='./src/**/*.js'",
    "watch:static": "cpx './src/static/**' dist -w",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "NODE_ENV=test jest --coverage"
  },
  "dependencies": {
    "element-ready": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@babel/plugin-external-helpers": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "babel-cli": "^7.0.0-beta.3",
    "babel-core": "^7.0.0-beta.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "browsersync": "^0.0.1-security",
    "cpx": "^1.5.0",
    "docdash": "^0.4.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "jest-css-modules": "^1.1.0",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.0",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-browsersync": "^0.2.6",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "sane": "^2.5.2"
  }
}
